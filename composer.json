{
  "name": "kokspflanze/zfc-sitemap",
  "description": "Sitemap module for Laminas, based on zend-navigation with eventmanager",
  "keywords": [
    "framework",
    "laminas",
    "zf",
    "navigation",
    "sitemap"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kokspflanze/ZfcSitemap",
  "autoload": {
    "psr-4": {
      "ZfcSitemap\\": "src"
    }
  },
  "require": {
    "php": "^7.1.0",
    "ext-json" : "*",
    "laminas/laminas-servicemanager": "^3.3.0",
    "laminas/laminas-eventmanager": "^3.2.1",
    "laminas/laminas-view": "^2.9.0",
    "laminas/laminas-mvc": "^3.1.0",
    "laminas/laminas-http": "^2.6.0",
    "laminas/laminas-stdlib": "^3.1.0",
    "laminas/laminas-router": "^3.0.2",
    "laminas/laminas-navigation": "^2.8.2"
  },
  "require-dev": {
    "laminas/laminas-mvc-console": "^1.0"
  },
  "extra": {
    "laminas": {
      "module": "ZfcSitemap"
    }
  }
}
